<form method="post"
      action="{{ path('app_demande_baby_sitter_delete', {'id': demande_baby_sitter.id}) }}"
      onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer cette demande ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ demande_baby_sitter.id) }}">
    <button class="btn btn-danger w-100 text-white">
        <i class="bi bi-trash me-1"></i>Supprimer
    </button>
</form>
