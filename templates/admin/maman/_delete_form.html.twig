<form method="post" action="{{ path('admin_maman_delete', { id: maman.id }) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer ce profil maman ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ maman.id) }}">
    <button type="submit" class="btn btn-outline-danger btn-sm"><i class="fas fa-trash me-1"></i> Supprimer</button>
</form>
