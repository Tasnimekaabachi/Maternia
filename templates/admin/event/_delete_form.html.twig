<form method="post" action="{{ path('app_event_delete', {'id': event.id}) }}" class="d-inline" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer cet élément ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ event.id) }}">
    <button type="submit" class="btn btn-danger">Supprimer</button>
</form>